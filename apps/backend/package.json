{
  "name": "@randar-market/backend",
  "version": "1.0.0",
  "description": "Randar Market Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:e2e": "cross-env NODE_ENV=test jest --runInBand",
    "lint": "eslint src/**/*.ts",
    "db:gen": "prisma generate --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "axios": "^1.6.0",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "node-cron": "^3.0.3",
    "ws": "^8.14.2",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "prom-client": "^15.1.0",
    "zod": "^3.22.4",
    "cookie": "^0.6.0",
    "@prisma/client": "^5.7.1",
    "lru-cache": "^10.1.0",
    "ioredis": "^5.3.2",
    "@randar-market/bot": "workspace:*",
    "@randar-market/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prisma": "^5.7.1",
    "cross-env": "^7.0.3"
  }
}
